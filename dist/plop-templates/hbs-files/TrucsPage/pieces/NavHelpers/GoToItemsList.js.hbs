import { Constants } from "src/constants/Constants";
import { SqliteReduxGangPageState } from "src/reduxState/GangPageState/GangPageStateGetterSetter";

/**
 * Let's go back to GangPageList
 */
export function GoToGangPageList() {
  // getter
  const GangPageState = SqliteReduxGangPageState.GetGangPageStateFirstRow();

  // setter
  SqliteReduxGangPageState.UpdateSpecificRowsFromDB({
    row: {
      ...GangPageState,

      // l'écran actuellement affiché dans GangPage.js
      chosenOne: "GangPageList",

      // cache le snack
      snackbarVisible: Constants.false,

      itemUniqueId: null,
    },
    rowName: "uniqueId",
    rowValue: "GangPageState",
    onSuccess: (row) => {
      /*console.log(
        `state de page modifé avec succès dans GangPage.`
      );*/
    },
    onError: (e) => {},
  });
}
