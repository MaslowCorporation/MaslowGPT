import { LogoutActions } from "../../AllActions/Logout/LogoutActions.js";
import { LogoutPrompts } from "../../AllPrompts/Logout/LogoutPrompts.js";
// eslint-disable-next-line no-unused-vars
import { CommandNames } from "../../AppConstants/CommandNames.js";

function initLogoutGenerator(plop, appRootPath) {
  /* PLOP_INJECT_CODE */

  plop.setGenerator(CommandNames.logout, {
    description: "Cette commande permet de .....",

    /**
     * on demande à l'utilisateur
     * divers informations permettant la
     * création de notre chose.
     *
     */
    prompts: LogoutPrompts(appRootPath),
    actions: function (data) {
      return LogoutActions(data, appRootPath, plop);
    },
  });
}

export { initLogoutGenerator };
